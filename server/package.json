    "@babel/preset-env": "^7.3.1",
    "concurrently": "^8.2.0",
    "connect-redis": "^7.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "lodash": "^4.17.11",
    "nodemon": "^2.0.22",
    "socket.io": "^4.7.1",
    "socket.io-redis": "^6.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "typescript": "^5.1.6"
  }
