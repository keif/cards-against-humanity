{
  "name": "cah_backend",
  "version": "0.1.0",
  "description": "CAH Backend",
  "main": "index.ts",
  "scripts": {
    "client": "cd ../client && yarn start",
    "server": "babel-node index.ts",
    "dev": "concurrently --kill-others-on-fail \\\"yarn server\\\" \\\"npm client\\\"",
    "build": "cd ../client && yarn && yarn run build",
    "start": "docker-compose up"
